<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Regression Test Suite" verbose="4" thread-count="3" parallel="tests">


	<listeners>
		<listener class-name="com.qa.opencart.listners.ExtentReportListener" />
		<listener class-name="com.qa.opencart.listners.TestAllureListener" />
		<listener class-name="com.qa.opencart.listners.AnnotationTransformer" />
	</listeners>


	<test name="Open Cart Login Page Test_91.0">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="91.0" />
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Open Cart Login Page Test_89.0">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="89.0" />
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Open Cart Login Page Test_100.0">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="100.0" />
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>

	<test name="Open Cart Accounts Page Test">
		<classes>
			<class name="com.qa.opencart.tests.AccountsPageTest" />
		</classes>
	</test>

	<test name="Open Cart Register Page Test">
		<classes>
			<class name="com.qa.opencart.tests.RegisterTest" />
		</classes>
	</test>

	<!-- <test name="Open Cart ProductComparison Page Test">
		<classes>
			<class name="com.qa.opencart.tests.ProductComparisonTest" />
		</classes>
	</test>-->


</suite>